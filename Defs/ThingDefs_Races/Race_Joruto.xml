<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!--Joruto = No space(PerfectlyNormalJoris) -->
    <!--Joruto = space(Perfectly Normal Joris) -->
    <!--JO = BodyPartDef(Base or BFJ) -->
    <AlienRace.ThingDef_AlienRace ParentName="JorisfolkRace">
        <defName>SW_Alien_Joruto</defName>
        <label>Joruto</label>
        <description>Joruto is a hyper-active and incredibly annoying Joris that yells at the most inappropriate moments, yet for reasons no scientist can explain, always seem to be able to endear itself to anyone it wants to. This naive ninja bear is capable of producing multiple copies of itself.</description>
        <comps>
            <li Class="AnimalBehaviours.CompProperties_AsexualReproduction">
                <reproductionIntervalDays>1</reproductionIntervalDays>
                <customString>Cloning in: </customString>
                <isGreenGoo>true</isGreenGoo>
                <GreenGooLimit>10</GreenGooLimit>
                <GreenGooTarget>SW_Alien_Joruto</GreenGooTarget>
            </li>
        </comps>
        <alienRace>
            <generalSettings>
                <alienPartGenerator>
                    <aliencrowntypes>
                        <li>JO</li>
                    </aliencrowntypes>
                    <alienbodytypes>
                        <li>JO</li>
                    </alienbodytypes>
                </alienPartGenerator>
            </generalSettings>
            <graphicPaths>
