                    <aliencrowntypes>
                        <li>EMJ</li>
                    </aliencrowntypes>
                    <alienbodytypes>
                        <li>EMJ</li>
                    </alienbodytypes>
                </alienPartGenerator>
                <forcedRaceTraitEntries>
                    <li>
                        <defName>NaturalMood</defName>
                        <degree>-2</degree>
                        <chance>100</chance>
                    </li>
                </forcedRaceTraitEntries>
