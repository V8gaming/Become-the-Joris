<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!--JockExcellente = No space(PerfectlyNormalJoris) -->
    <!--Jock Excellente = space(Perfectly Normal Joris) -->
    <!--JE = BodyPartDef(Base or BFJ) -->
    <AlienRace.ThingDef_AlienRace ParentName="JorisfolkRace">
        <defName>SW_Alien_JockExcellente</defName>
        <label>Jock Excellente</label>
        <description>A stylish and charismatic Joris who entrances colonists with his suave and seductive ways, but shares a bitter rivalry with other species of Excellente Joris'. Jock Excellente is known for his keen fashion sense, sporting a look that would make even the Stellarch jealous.</description>
        <race>
            <useMeatFrom>JE_PerfectlyNormalJoris</useMeatFrom>
        </race>
        <comps>
            <li Class="AnimalBehaviours.CompProperties_InitialHediff">
                <hediffname>JE_NoPainLowRest</hediffname>
                <hediffseverity>1</hediffseverity>
            </li>
            <li Class="AnimalBehaviours.CompProperties_Regeneration">
                <rateInTicks>20</rateInTicks>

            </li>
            <li Class="AnimalBehaviours.CompProperties_ThoughtEffecter">
                <radius>15</radius>
                <tickInterval>1500</tickInterval>
                <thoughtDef>JE_LookAtTheMuscles</thoughtDef>
            </li>
        </comps>
        <alienRace>
            <generalSettings>
                <alienPartGenerator>
                    <aliencrowntypes>
                        <li>JE</li>
                    </aliencrowntypes>
                    <alienbodytypes>
                        <li>JE</li>
                    </alienbodytypes>
                </alienPartGenerator>
            </generalSettings>
            <graphicPaths>
                <li>
                    <lifeStageDefs>
                        <li>AdultJorisStage</li>
                    </lifeStageDefs>
                    <body>Pawn/JockExcellente/Body/</body>
                    <head>Pawn/JockExcellente/Head/</head>
                    <skeleton>Pawn/PerfectlyNormalJoris/Body/Rotten</skeleton>
                    <skull>Pawn/PerfectlyNormalJoris/Head/Skull</skull>
                    <customDrawSize>(1.7, 1.7)</customDrawSize>
                    <customPortraitDrawSize>(1.2, 1.2)</customPortraitDrawSize>
                    <customHeadDrawSize>(1.7, 1.7)</customHeadDrawSize>
                    <customPortraitHeadDrawSize>(1.2, 1.2)</customPortraitHeadDrawSize>
                    <headOffsetDirectional>
                        <north>(-0.01400560224, 0.7887532371)</north>
                        <east>(1.183869774, 0.28)</east>
                        <south>(-0.00892494929, -0.05546218487)</south>
                        <west>(-1.183869774, 0.28)</west>
                    </headOffsetDirectional>
                </li>
                <li>
                    <lifeStageDefs>
                        <li>TeenJorisStage</li>
                    </lifeStageDefs>
                    <body>Pawn/JockExcellente/Body/</body>
                    <head>Pawn/JockExcellente/Head/</head>
                    <skeleton>Pawn/PerfectlyNormalJoris/Body/Rotten</skeleton>
                    <skull>Pawn/PerfectlyNormalJoris/Head/Skull</skull>
                    <customDrawSize>(1, 1)</customDrawSize>
                    <customPortraitDrawSize>(1.2, 1.2)</customPortraitDrawSize>
                    <customHeadDrawSize>(1, 1)</customHeadDrawSize>
                    <customPortraitHeadDrawSize>(1.2, 1.2)</customPortraitHeadDrawSize>
                    <headOffsetDirectional>
                        <north>(0.002380952381, 0.4496855346)</north>
                        <east>(0.7044025157, 0.1052631579)</east>
                        <south>(-0.004022988506, -0.025)</south>
                        <west>(-0.7044025157, 0.1052631579)</west>
                    </headOffsetDirectional>
                </li>
                <li>
                    <lifeStageDefs>
                        <li>BabyJorisStage</li>
                    </lifeStageDefs>
                    <body>Pawn/JockExcellente/Body/</body>
                    <head>Pawn/JockExcellente/Head/</head>
